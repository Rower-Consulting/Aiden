<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="700" d:DesignHeight="650"
             xmlns:vm="using:AidenDesktop.ViewModels"
             Design.DataContext="{Binding Source={x:Static vm:SettingsViewModel.DesignVM}}"
             x:DataType="vm:SettingsViewModel"
             x:CompileBindings="True"
             x:Class="AidenDesktop.Views.SettingsView">
    <StackPanel HorizontalAlignment="Center" Margin="10">
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Settings View " FontSize="32" Margin="32" Foreground="Tan"/>
        </StackPanel>

    </StackPanel>
</UserControl>